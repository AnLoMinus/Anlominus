    echo "  ${cWall}  â• â•â–¶ Creating $grey Forensic"
    mkdir "11 - Forensic"
    sleep 1
    # Information
    echo "  ${cWall}  â• â•â–¶ Creating $grey Reporting"
    mkdir "12 - Reporting"
    sleep 1
    # Information
    echo "  ${cWall}  â• â•â–¶ Creating $grey Social Engineering"
    mkdir "13 - Social Engineering"
    sleep 1
    # Information
    echo "  ${cWall}  â• â•â–¶ Creating $grey Privilege Escalation"
    mkdir "14 - Privilege Escalation"
    sleep 1
    # Information
    echo "  ${cWall}  â• â•â–¶ Creating $grey Malware Analysis"
    mkdir "15 - Malware Analysis"
    sleep 1
    # Information
    echo "  ${cWall}  â• â•â–¶ Creating $grey Covering Tracks"
    mkdir "16 - Covering Tracks"
    sleep 1
    # Information
    echo "  ${cWall}  â•šâ•â–¶ ${currentDir}"
    ReadSomthing
    # chmod +x Install
    # sh Install
    ;;
    r|remove|R|Remove)
    clear
    Banner
    if [ -e AnlominusCore ]; then
      #statements
      echo "  ${cWall}  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  ${cWall}"
      echo "  ${cWall}  â• â•>             Anlominus            <==â•‘  ${cWall}"
      echo "  ${cWall}  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  ${cWall}"
      sleep 0.5
      echo "  ${cWall}  â• â•âž¤ ${cInfo} Found AnlominusCore....            ${cWall}"
      sleep 1.5
      echo "  ${cWall}  â• â•âž¤ $cDanger Removing Anlominus Tools....       ${cWall}"
      sleep 2
      `rm -R AnlominusCore*`
      echo "  ${cWall}  â•šâ•âž¤ $cDone Removing Complete....              ${cWall}"
      sleep 2
    else
      clear
      Banner
      echo "  $cFalse  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cFalse "
      echo "  $cFalse  â• â•>             Anlominus            <==â•‘  $cFalse "
      echo "  $cFalse  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cFalse "
      echo "  $cFalse  â•šâ•âž¤ $cFalse AnlominusCore not exist... $cFalse     $cFalse "
      sleep 1.5
    fi
    ;;
    t|todo|T|TODO)
    chmod +x TODO
    sh TODO
    ;;
    x|X|exit|Exit)
    clear
    Banner
    Exiting
    sleep 0.3
    echo ""
    break
    ;;
    ab|about|About)
    clear
    Banner
    echo "  $cWallâ–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘$cWall"
    echo "  $cWallâ–‘  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  â–‘$cWall"
    echo "  $cWallâ–‘  â• â•>      $cFalse About Anlominus $cFalse     <==â•‘  â–‘$cWall"
    echo "  $cWallâ–‘  â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  â–‘$cWall"
    echo "  $cWallâ–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘â–‘$cWall"
    echo ""
    chmod +x "About"
    sh "About"
    ;;
    1)
    chmod +x CheckOS
    sh CheckOS
    ;;
    *)
    invalidFunction
    ;;
  esac
done
