      echo "  $cWall░  ╚═══════════════════════════════════════╝  ░$cWall"
      echo "  $cWall░                                             ░$cWall"
      echo "  $cWall░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░$cWall"
      echo ""
      sleep 1
      clear
      ;;
    esac
  done
}
