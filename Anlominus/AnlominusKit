  $cWall▓  ╠═>       AnloMinus  HacKingPro      <==║  ▓$cWall
  $cWall▓  ╠═══════════════════════════════════════╝  ▓$cWall
  $cWall▓  ╠═▶ ${c0} - ${green}Anonymity                        ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c1} - ${green}Information Gathering            ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c2} - ${green}Vulnerability Analysis           ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c3} - ${green}Web Application Analysis         ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c4} - ${green}Database Assessment              ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c5} - ${green}Password Attacks                 ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c6} - ${green}Wireless Attacks                 ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c7} - ${green}Reverse Engineering              ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c8} - ${green}Exploit Frameworks               ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c9} - ${green}Sniffing - Spoofing              ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c10} - ${green}Maintaining Access              ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c11} - ${green}Forensic                        ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c12} - ${green}Reporting                       ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c13} - ${green}Social Engineering              ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c14} - ${green}Privilege Escalation            ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c15} - ${green}Malware Analysis                ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c16} - ${green}Covering Tracks                 ${cyan}▓$cWall
  $cWall▓  ╠═▶ ${c17} - ${green}FireWall Tools                  ${cyan}▓$cWall
  $cWall▓  ╠                                          ▓$cWall
  $cWall▓  ╚═▶ ${cx} - ${green}Exit                             ${cyan}▓$cWall
  $cWall▓                                             ▓$cWall
  $cWall▓                                             ▓$cWall
  $cWall▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓$cWall"
  PromptFun
    read cmd
    case ${cmd} in
      x|X|exit|Exit)
      clear
      Banner
      Exiting
      sleep 0.3
      echo ""
      break
      ;;
      1)
      chmod +x AnlominusCheckOS
      sh AnlominusCheckOS
      ReadSomthing
      ;;
      17)
      chmod +x AnlominusFireWall
      sh AnlominusFireWall
      ReadSomthing
      ;;
      14)
      chmod +x AnlominusPrivEsc
      sh AnlominusPrivEsc
      ;;
      *)
      clear
      Banner
      echo ""
      echo "  $cWall░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░$cWall"
      echo "  $cWall░                                             ░$cWall"
      echo "  $cWall░  ╔═══════════════════════════════════════╗  ░$cWall"
