ALMBar="${orange} â”€â”€[${blue}Anlominus${purple}@${red}PrivEsc${cyan}$~${orange}]â”€â”€"

AnlominusPrivEsc(){
  while true; do
    clear
    BannerMan
    echo "  $cWallâ–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“$cWall"
    echo "  $cWallâ–“                                             â–“$cWall"
    echo "  $cWallâ–“  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  â–“$cWall"
    echo "  $cWallâ–“  â• â•>  ${ALMBar}${cyan}   <==â•‘  â–“$cWall"
    echo "  $cWallâ–“  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ ${c0} - ${green}Linux System                     ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ ${c1} - ${green}Linux User and groups            ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ ${c2} - ${green}Linux Networks                   ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ ${c3} - ${green}Linux Job and Tasks              ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ ${c4} - ${green}Linux SUID and GUID              ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ ${c5} - ${green}Linux Services                   ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶ $cx ${green}Exit                               ${cyan}â–“$cWall"
    echo "  $cWallâ–“  â•šâ•âž¤ $promptOption                                       ${cyan}â–“$cWall"
    PromptFun
    read cmd
    case ${cmd} in
      0)
      AnlominusSystem
      echo "
      $cInfo#############################################$cInfo
      $cInfo                                             $cInfo
      $cInfo  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cInfo
      $cInfo  â• â•>    PRESS ENTER TO GOING BACK     <==â•‘  $cInfo
      $cInfo  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cInfo"
      echo -n "      $cInfo  â•šâ•âž¤ "
      read read_somthing
      ;;
      1)
      AnlominusUsersGroups
      echo "
      $cInfo#############################################$cInfo
      $cInfo                                             $cInfo
      $cInfo  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cInfo
      $cInfo  â• â•>    PRESS ENTER TO GOING BACK     <==â•‘  $cInfo
      $cInfo  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cInfo"
      echo -n "      $cInfo  â•šâ•âž¤ "
      read read_somthing
      ;;
      2)
      AnlominusNetworks
      echo "
      $cInfo#############################################$cInfo
      $cInfo                                             $cInfo
      $cInfo  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cInfo
      $cInfo  â• â•>    PRESS ENTER TO GOING BACK     <==â•‘  $cInfo
      $cInfo  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cInfo"
      echo -n "      $cInfo  â•šâ•âž¤ "
      read read_somthing
      ;;
      3)
      AnlominusJobsTasks
      echo "
      $cInfo#############################################$cInfo
      $cInfo                                             $cInfo
      $cInfo  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cInfo
      $cInfo  â• â•>    PRESS ENTER TO GOING BACK     <==â•‘  $cInfo
      $cInfo  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cInfo"
      echo -n "      $cInfo  â•šâ•âž¤ "
      read read_somthing
      ;;
