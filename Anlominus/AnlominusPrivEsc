      $cInfo  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cInfo
      $cInfo  â• â•>    PRESS ENTER TO GOING BACK     <==â•‘  $cInfo
      $cInfo  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cInfo"
      echo -n "      $cInfo  â•šâ•âž¤ "
      read read_somthing
      ;;
      x|X|exit|Exit)
      clear
      Banner
      BannerExit
      sleep 0.3
      echo ""
      break
      ;;
      *)
      clear
      Banner
      echo "  $cFalse  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  $cFalse "
      echo "  $cFalse  â• â•>      $cFalse invalid option!! $cFalse    <==â•‘  $cFalse "
      echo "  $cFalse  â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  $cFalse "
      echo ""
      sleep 1
      clear
      ;;
    esac
  done
}
